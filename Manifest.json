{
    "manifest_version": 3,
    "name": "Ashu Music Player",
    "description": "Get a new song every day",
    "version": "0.0.1",
    "action": {
        "default_popup": "index.html",
        "default_icon": "logo.png"
    },
    "icons": {
        "128": "logo.png"
    },
    "permissions": [
        "activeTab"
    ]
}